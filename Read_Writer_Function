def data_transformations (data_source):
      if (type(data_source) != type(dict)) or (type(data_source) != type(collections.OrderedDict())):
         raise TypeError ('Type needs to be a dictionary')
    dataframe = pd.DataFrame()
    for key, value in data_source.items():
        #print(value.head(10))
        value['Month'] = key
        dataframe = pd.concat([dataframe,value])
    return dataframe

def combine_data(data_sources,data_names):
    if len(data_sources) != len(data_names):
        raise ValueError ('Should have an equal count of names to sources')
    output = {}
    for name_, source in zip(data_names, data_sources):
        output[name_] = data_transformations(source)
    Final_output = pd.concat(output)
    Final_output.set_index('Month', append=True, inplace=True)
    Final_output.to_excel('File Path' ,sheet_name='FLP')
